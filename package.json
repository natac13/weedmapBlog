{
  "name": "weedmaps-blog",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "lint": "eslint client/ && eslint server",
    "work:start": "echo 'START' >> workLogfile.txt && date -d now >> workLogfile.txt && git add workLogfile.txt && git cm",
    "work:end": "echo 'END' >> workLogfile.txt && date -d now >> workLogfile.txt && git add workLogfile.txt && git cm"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "meteor-node-stubs": "~0.2.0",
    "meteoredux": "0.0.2",
    "normalize.css": "4.1.1",
    "ramda": "0.21.0",
    "react": "15.1.0",
    "react-addons-css-transition-group": "15.1.0",
    "react-addons-pure-render-mixin": "15.0.2",
    "react-dom": "15.0.2",
    "react-redux": "4.4.5",
    "react-router": "2.4.1",
    "react-router-redux": "4.0.4",
    "react-toolbox": "0.16.2",
    "recompose": "0.19.0",
    "redux": "3.5.2",
    "redux-actions": "0.9.1",
    "redux-form": "5.2.4",
    "redux-logger": "2.6.1"
  },
  "devDependencies": {
    "eslint": "2.10.2",
    "eslint-config-natac13": "1.3.0"
  },
  "description": "test project",
  "version": "1.0.0",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/natac13/weedmapBlog.git"
  },
  "keywords": [
    "meteor",
    "react",
    "redux"
  ],
  "author": "Sean Campbell <sean.campbell13@gmail.com> (http://seancampbellnatac.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/natac13/weedmapBlog/issues"
  },
  "homepage": "https://github.com/natac13/weedmapBlog#readme",
  "cssModules": {
    "extensions": [
      "scss"
    ],
    "globalVariables": [
      "node_modules/react-toolbox/lib/_colors.scss",
      {
        "theme-building": true
      },
      "client/toolbox-theme.scss"
    ],
    "outputJsFilePath": {
      "node_modules/react-toolbox/.*": "{dirname}/{basename}"
    }
  }
}
